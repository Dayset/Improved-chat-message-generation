function make_message(e){if("MESSAGE"==e.type||"TIPS"==e.type||is_album_message_type(e.type)){e.text=e.text.replace(RegExp("<([1-9]|[1-2][0-9]|3[0-5])>","g"),"<img border=0 width=30px src='https://github.com/Dayset/Improved-chat-message-generation/raw/main/smileys/$1.gif'>"),e.text=e.text.replace(RegExp("<(\\d+)>","g"),"<img border=0 src=/img/smiles/smile$1.gif>"),e.text=e.text.replace(RegExp("йух","g"),"хуй");var t="color:#"+e.color+"; font-weight:bold;",i="color:#"+e.color+";",r=e.time.substr(11,8),a="",s=!1;if(0==e.is_private){a="<span class='"+("f"==e.user_sex?"time_girl":"time_boy")+"'>("+r+")</span>",0<e.user_id&&(a+=make_dropdown_menu_to_user(e,!1),a+="<a style='"+t+";text-decoration:underline;' href='javascript:writeName(\""+e.user_nick.replace(/&quot;/g,'\\"')+"\")'>"+e.user_nick+"</a>"),a+="<span style='"+i+"'>: ";let l=e.text.split(" ");l.forEach(e=>{/^[a-zA-Z0-9_-]{11}$/.test(e)&&!s?(a+=`<iframe src="https://www.youtube.com/embed/${e}"width="512"height="32"frameborder="0"style="display:inline-block; vertical-align:middle; margin-left:5px;"allow="autoplay;encrypted-media"allowfullscreen></iframe>`,s=!0):a+=e+" "}),a+="</span>"}else if(a="<span class='"+("f"==e.user_sex?"time_girl":"time_boy")+"'>("+r+")</span>",0<e.user_id){a+=make_dropdown_menu_to_user(e,!0),r=escapeQuot(escape_user_nick(e.user_nick)),a+="<a style='"+t+";text-decoration:underline;' href='javascript:header_send_private("+e.user_id+', "'+r+"\")'>"+e.user_nick+"</a>";let o=e.text.split(" ");o.forEach(e=>{/^[a-zA-Z0-9_-]{11}$/.test(e)&&!s?(a+=`<iframe src="https://www.youtube.com/embed/${e}"width="512"height="32"frameborder="0"style="display:inline-block;vertical-align:middle;margin-left:5px;"allow="autoplay;encrypted-media"allowfullscreen></iframe>`,s=!0):a+=e+" "})}}return"READY"==e.type&&(a=e.text),a}
